{
  "name": "voxylauncher",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build:electron": "tsc -p electron",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "prod": "vite build && tsc -p electron && electron-builder"
  },
  "dependencies": {
    "@material-tailwind/react": "^3.0.0-beta.24",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.8.1",
    "framer-motion": "^12.4.7",
    "keytar": "^7.9.0",
    "minecraft-launcher-core": "^3.18.2",
    "msmc": "^5.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tailwindcss/postcss": "^4.0.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "sass-embedded": "^1.83.4",
    "tailwindcss": "^4.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.zk.voxylauncher",
    "productName": "VoxyLauncher",
    "directories": {
      "output": "win"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "src/assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/icon.png"
    }
  }
}
